{#
/**
 * @file
 * View modes inventory - Horizontal media teaser - xlarge text only template.
 */
#}
<{{ outer_wrapper }}{{ attributes.addClass('horizontal-media-teaser-view-mode', 'xlarge-text-only', 'clearfix') }}>
  {% if title_suffix.contextual_links is not null %}
    {{ title_suffix.contextual_links }}
  {% endif %}
  <{{ main_wrapper }} class="main-content">
    {{ content.main }}
  </{{ main_wrapper }}>
</{{ outer_wrapper }}>
