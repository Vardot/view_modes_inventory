<{{ outer_wrapper }}{{ attributes.addClass('text-teaser-view-mode', 'small', 'clearfix') }}>
  {% if title_suffix.contextual_links is not null %}
    {{ title_suffix.contextual_links }}
  {% endif %}
  <{{ main_wrapper }} class="main-content">
    {{ content.main }}
  </{{ main_wrapper }}>
</{{ outer_wrapper }}>
